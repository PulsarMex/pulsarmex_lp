---
import Layout from "../layouts/Layout.astro";
import Header from "../components/Header.astro";
import Footer from "../components/Footer.astro";
---

<Layout>
  <Header />
  <div
    class="container min-h-screen mx-auto grid grid-cols-1 md:grid-cols-2 gap-8 items-start py-15"
  >
    <div class="flex justify-end xl:sticky xl:top-0 xl:overflow-auto">
      <img
        src="/dedc_pic.png"
        class="w-full max-w-2xl drop-shadow-2xl profile-pic lg:scale-100 md:scale-125"
        loading="lazy"
      />
    </div>
    <div class="p-10 md:p-0 lg:pr-10">
      <h1 class="font-extrabold text-5xl">David Ernesto Domínguez Castro</h1>
      <h3 class="text-lg mt-2">
        Desarrollador Web Full-Stack en Django/Python
      </h3>
      <h3 class="text-lg">
        Maestría en Administración de Tecnologías de la Información
      </h3>
      <h3 class="text-lg">Ingeniería en Tecnologías de la Información</h3>
      <h3 class="text-lg mb-3">TSU en Sistemas Informáticos</h3>

      <a
        href="https://www.linkedin.com/in/david-ernesto-dc-5a3330ab/"
        target="_blank"
        class="btn btn-sm bg-[#0967C2] text-white border-[#0059b3] me-1 mt-2"
      >
        <svg
          aria-label="LinkedIn logo"
          width="16"
          height="16"
          xmlns="http://www.w3.org/2000/svg"
          viewBox="0 0 32 32"
          ><path
            fill="white"
